<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="316.0" prefWidth="470.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.PrimaryController">
   <children>
      <Label text="Oversetter" />
      <CheckBox fx:id="bokmålBoolean" mnemonicParsing="false" text="Til nynorsk fra bokmål?" />
      <SplitPane prefHeight="160.0" prefWidth="200.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="157.0" prefWidth="428.0">
               <children>
                  <TextArea fx:id="inputText" layoutX="14.0" layoutY="20.0" prefHeight="118.0" prefWidth="248.0" promptText="Skriv inn her:" wrapText="true" />
                  <Label fx:id="outputLabel" layoutX="272.0" layoutY="20.0" maxWidth="200" text="Output:" wrapText="true" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
      <Button fx:id="primaryButton" onAction="#translateText" text="Oversett" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
