<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="337.0" prefWidth="470.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.PrimaryController">
   <children>
      <Label text="Oversetter" />
      <CheckBox mnemonicParsing="false" text="Bokmål input" fx:id="bokmålBoolean"/>
      <SplitPane prefHeight="160.0" prefWidth="200.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="170.0" prefWidth="428.0">
               <children>
                  <TextArea fx:id="inputText" layoutX="33.0" layoutY="14.0" prefHeight="118.0" prefWidth="204.0" promptText="Skriv inn her:" />
                  <Label layoutX="281.0" layoutY="71.0" text="Output:" fx:id="outputLabel"/>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
      <Button fx:id="primaryButton" onAction="#translateText" text="Oversett" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
