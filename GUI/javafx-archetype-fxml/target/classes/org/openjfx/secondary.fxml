<VBox alignment="CENTER" maxHeight="320.0" maxWidth="470.0" prefHeight="320.0" prefWidth="470.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.SecondaryController">
    <children>
        <Label text="Brukermanual" />
      <AnchorPane maxHeight="210.0" maxWidth="254.0" prefHeight="186.0" prefWidth="254.0">
         <children>
            <Label layoutX="15.0" prefHeight="72.0" prefWidth="221.0" text="Når &quot;Bokmål til nynorsk&quot; ikke er merket vil programmet oversette fra nynorsk til bokmål isteden." wrapText="true" />
            <Label layoutX="17.0" layoutY="72.0" prefHeight="72.0" prefWidth="221.0" text="Vær obs på at programmet ikke tar hensyn til unntak ved språk, eller tilfeller der setningstruktur er anderledes. Se over oversettelsen" wrapText="true" />
            <Label layoutX="17.0" layoutY="150.0" prefHeight="34.0" prefWidth="221.0" text="Dersom ord er skrevet feil vil programmet ikke gjenkjenne dette" wrapText="true" />
         </children>
      </AnchorPane>
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Tilbake til Oversetter" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
